\";\n            this.context.toggleGameover();\n            alert(\"Boom! Gameover!\");\n        }\n        \n        this.setState({content: content});    \n        console.log(\"Update \");\n        console.log(content);\n    }\n    //*************** */\n    // User actions\n    //************** */\n    openCell = () => {\n        if (this.state.isOpened) return;\n        if (this.context.isGameover) return;\n        console.log(\"Open cell\");\n        this.setState({isOpened: true});\n        this.onOpenHandler();\n    }\n    toggleFlag = () => {\n        this.setState({isFlag: !this.state.isFlag});        \n    }\n    flagCell = (e) => {\n        e.preventDefault(); // Prevent open context menu\n        if (this.state.isOpened) return;\n        this.toggleFlag();\n        this.onGame();\n    }\n    componentWillUpdate(nextProps,abc, a1, b1) {\n        let context = this.context;\n        if (this.state.isFlag) {\n            this.setState({isFlag: false});\n        }\n        \n        if (nextProps !== this.props) {\n            this.setState({...nextProps})\n        }\n        let {i, j} = this.props;\n        let idx = i+j*this.context.level.width;\n        // TODO: Loop update twice\n        console.log(idx);\n        if (context.isGameover && context.selectedCell == idx) {\n            console.log(\"game over\");\n            this.setState({isOpened: true});\n            context.toggleGameover();\n        }\n        if (context && context.isRestart && this.state.isOpened) {\n            console.log(\"Restart\");\n            this.setState({isOpened: false, });\n        }\n    }\n    render() {\n        let {content, isOpened, isFlag} = this.state;\n        if (this.context.isGameover) {\n            isOpened = true;\n        }\n        if (!isOpened) {\n            content=isFlag?\"F\":\"\";\n        }\n        return (\n                <div \n                className=\"cell\"  \n                onClick={this.openCell}\n                onContextMenu={this.flagCell}>\n                    <span>{content}</span>\n                </div>\n        )\n    }\n}\nCell.contextType = AppContext;\n\nexport default Cell\n","/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/constants.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/stores/levels.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/components/MenuApp/Timer/index.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/components/MenuApp/RestartButton/index.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/components/MenuApp/BombCounter/index.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/stores/stateGameover.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/stores/stateRestart.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/actions/state.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/actions/levels.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/components/Theme/ThemedButton.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/components/Theme/ThemeContext.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/AppContext.js",[],"/media/dainguyen/DATA/Project/ReactJS/Game/Minesweeper/src/components/RestartButton/index.js",[],{"ruleId":"91","severity":1,"message":"92","line":41,"column":10,"nodeType":"93","messageId":"94","endLine":41,"endColumn":19},{"ruleId":"91","severity":1,"message":"95","line":41,"column":21,"nodeType":"93","messageId":"94","endLine":41,"endColumn":31},{"ruleId":"96","severity":1,"message":"97","line":19,"column":25,"nodeType":"98","messageId":"99","endLine":19,"endColumn":27},{"ruleId":"91","severity":1,"message":"100","line":127,"column":22,"nodeType":"93","messageId":"94","endLine":127,"endColumn":31},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"91","severity":1,"message":"105","line":47,"column":30,"nodeType":"93","